export ZOPEN_TARBALL_DEPS="make curl makeinfo perl m4"
export ZOPEN_TARBALL_URL="https://ftp.gnu.org/gnu/groff/groff-1.22.4.tar.gz"
export ZOPEN_TYPE="TARBALL"
export ZOPEN_EXTRA_CONFIGURE_OPTS="--with-x=no"
export ZOPEN_EXTRA_CFLAGS='-U_MI_BUILTIN'
export ZOPEN_EXTRA_CXXFLAGS='-U_MI_BUILTIN'
export ZOPEN_CHECK="skip"

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  # Echo the following information to guage build health
  echo "actualFailures:0"
  echo "totalTests:1"
  echo "expectedFailures:0"
}

zopen_append_to_env()
{
  # echo envars outside of PATH, MANPATH, LIBPATH
}
